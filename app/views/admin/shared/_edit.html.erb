<% className = form_action.delete(:class) %>
<%= form_tag(form_action, :id => "#{form_type}_form", :enctype => "multipart/form-data", :class => className) do %>
  <%= render :partial => "form" %>
<% end %>
<% id=@article.id if @article %>
<% id=@page.id if @page %>
<% form_tag("/admin/content/merge_with/#{id}", :method=>"post") do %>
	<%= render :partial => "admin/shared/merge_article" %>
<% end %>